FoamFile
{
    version     2.0;
    format      binary;
    class       pointVectorField;
    location    "0";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    top
    {
        type            fixedNormalSlip;
        n               (0 0 1);
    }

    sides
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    bottom
    {
        type            fixedNormalSlip;
        n               (0 0 1);
    }
    wing
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


